{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"files": {
		"ignore": [
			"./target",
			"./end2end/playwright-report",
			"./end2end/test-results"
		]
	},
	"organizeImports": {
		"enabled": false
	},
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"correctness": {
				"noUnusedImports": {
					"level": "error",
					"fix": "none"
				}
			}
		}
	}
}
